  "scripts": {
    "dev": " nodemon --exec ts-node src/server.ts",
    "start": "ts-node src/server.ts",
    "typeorm": "npx ts-node ./node_modules/typeorm/cli.js",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migration:drop-create": "ts-node ./node_modules/typeorm/cli.js migration:drop && ts-node",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run ",
    "migration:show": "ts-node ./node_modules/typeorm/cli.js migration:show",
    "migration:rollback": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^8.0.29",
    "@types/yup": "^0.29.13",
    "nodemon": "^2.0.8",
    "sucrase": "^3.19.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/typescript": "^2.0.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.18.1",
    "date-fns": "^2.0.0-beta.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.3",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.34",
    "yup": "^0.32.9"
  }
}
